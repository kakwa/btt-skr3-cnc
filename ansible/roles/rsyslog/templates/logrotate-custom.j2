# This file is managed by Ansible - DO NOT EDIT MANUALLY
# Logrotate configuration for {{ item.name }}

{{ item.path }} {
{% for option in item.options | default([]) %}
    {{ option }}
{% endfor %}
{% if item.postrotate is defined %}
    postrotate
        {{ item.postrotate }}
    endscript
{% endif %}
{% if item.prerotate is defined %}
    prerotate
        {{ item.prerotate }}
    endscript
{% endif %}
}
